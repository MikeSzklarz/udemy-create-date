{
  "manifest_version": 3,
  "name": "Udemy Creation Date",
  "version": "1.0",
  "description": "Displays the course creation date on Udemy",
  "host_permissions": ["https://www.udemy.com/api-2.0/*"],
  "content_scripts": [
    {
      "matches": ["https://www.udemy.com/course/*"],
      "js": ["scripts/content.js"]
    }
  ]
}
